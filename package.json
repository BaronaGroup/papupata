{
  "name": "papupata",
  "version": "0.0.1",
  "peerDependencies": {
    "@types/express": "^4"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/jest": "^24.0.15",
    "@types/lodash": "^4.14.135",
    "@types/request-promise": "^4.1.44",
    "express": "^4.17.1",
    "gulp": "^4.0.2",
    "gulp-clean": "^0.4.0",
    "gulp-typescript": "^5.0.1",
    "jest": "^24.8.0",
    "prettier": "^1.18.2",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.2"
  },
  "dependencies": {
    "lodash": "^4.17.11"
  },
  "main": "dist/main/index",
  "scripts": {
    "build": "gulp",
    "test": "jest"
  },
  "jest": {
    "collectCoverageFrom": [
      "<rootDir>src/**/*.ts"
    ],
    "testMatch": [
      "<rootDir>/src/**/*-test.ts"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "testEnvironment": "node",
    "globals": {
      "ts-jest": {
        "tsConfig": {
          "incremental": true
        }
      }
    }
  }
}
