{
  "name": "papupata",
  "version": "1.6.0",
  "description": "Typescript-typed REST requests and API implementations",
  "homepage": "https://baronagroup.github.io/papupata",
  "repository": {
    "type": "git",
    "url": "BaronaGroup/papupata"
  },
  "license": "MIT",
  "author": {
    "name": "Tapani Haka"
  },
  "peerDependencies": {
    "@types/express": "^4"
  },
  "devDependencies": {
    "@types/body-parser": "1.19.0",
    "@types/express": "4.17.4",
    "@types/jest": "25.2.1",
    "@types/lodash": "4.14.150",
    "@types/qs": "6.9.2",
    "@types/request-promise": "4.1.46",
    "@types/supertest": "2.0.9",
    "body-parser": "1.19.0",
    "express": "4.17.1",
    "gulp": "4.0.2",
    "gulp-clean": "0.4.0",
    "gulp-typescript": "5.0.1",
    "jest": "26.0.1",
    "npm-release": "1.0.0",
    "prettier": "2.0.5",
    "request": "2.88.2",
    "request-promise": "4.2.5",
    "supertest": "4.0.2",
    "ts-jest": "25.4.0",
    "typescript": "3.8.3"
  },
  "dependencies": {
    "lodash": "^4.17.11",
    "qs": "^6.7.0"
  },
  "main": "dist/main/index",
  "types": "dist/main/index.d.ts",
  "scripts": {
    "build": "gulp",
    "test": "jest",
    "prepare": "npm run build",
    "release": "npm test && npm-release"
  },
  "jest": {
    "collectCoverageFrom": [
      "<rootDir>src/**/*.ts"
    ],
    "testMatch": [
      "<rootDir>/src/**/*-test.ts"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "testEnvironment": "node",
    "globals": {
      "ts-jest": {
        "tsConfig": {
          "incremental": true
        }
      }
    }
  }
}
