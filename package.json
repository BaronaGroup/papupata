{
  "name": "papupata",
  "version": "1.0.0",
  "description": "Typescript-typed REST requests and API implementations",
  "homepage": "https://baronagroup.github.io/papupata",
  "repository": {
    "type": "git",
    "url": "BaronaGroup/papupata"
  },
  "license": "MIT",
  "author": {
    "name": "Tapani Haka"
  },
  "peerDependencies": {
    "@types/express": "^4"
  },
  "devDependencies": {
    "@types/body-parser": "1.17.1",
    "@types/express": "4.17.1",
    "@types/jest": "24.0.18",
    "@types/lodash": "4.14.137",
    "@types/qs": "6.5.3",
    "@types/request-promise": "4.1.44",
    "body-parser": "1.19.0",
    "express": "4.17.1",
    "gulp": "4.0.2",
    "gulp-clean": "0.4.0",
    "gulp-typescript": "5.0.1",
    "jest": "24.9.0",
    "npm-release": "1.0.0",
    "prettier": "1.18.2",
    "request": "2.88.0",
    "request-promise": "4.2.4",
    "ts-jest": "24.0.2",
    "typescript": "3.6.2"
  },
  "dependencies": {
    "lodash": "^4.17.11",
    "qs": "^6.7.0"
  },
  "main": "dist/main/index",
  "types": "dist/main/index.d.ts",
  "scripts": {
    "build": "gulp",
    "test": "jest",
    "prepare": "npm run build",
    "release": "npm test && npm-release"
  },
  "jest": {
    "collectCoverageFrom": [
      "<rootDir>src/**/*.ts"
    ],
    "testMatch": [
      "<rootDir>/src/**/*-test.ts"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "testEnvironment": "node",
    "globals": {
      "ts-jest": {
        "tsConfig": {
          "incremental": true
        }
      }
    }
  }
}
