{
  "name": "papupata-from-openapi",
  "version": "0.0.3",
  "description": "Generates papupata API declarations from OpenAPI documents",
  "main": "dist/main/generate",
  "types": "dist/main/generate.d.ts",
  "repository": {
    "type": "git",
    "url": "BaronaGroup/papupata"
  },
  "scripts": {
    "test": "jest",
    "build": "tsc --project tsconfig.json",
    "clean": "rm -rf ./dist",
    "prepare": "if [ -e node_modules ]; then npm run build; fi",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "license": "MIT",
  "author": {
    "name": "Tapani Haka"
  },
  "dependencies": {
    "@apidevtools/swagger-parser": "10.1.0",
    "openapi-types": "12.1.3"
  },
  "files": [
    "dist"
  ],
  "bin": "dist/main/app.js",
  "devDependencies": {
    "@types/jest": "27.0.2",
    "jest": "27.2.3",
    "ts-jest": "27.0.5",
    "ts-node": "10.2.1",
    "typescript": "4.9.5"
  },
  "jest": {
    "collectCoverageFrom": [
      "<rootDir>src/**/*.ts"
    ],
    "testMatch": [
      "<rootDir>/src/**/*-test.ts"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "testEnvironment": "node",
    "globals": {
      "ts-jest": {
        "tsConfig": {
          "incremental": true
        }
      }
    }
  }
}
